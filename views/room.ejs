<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script>
        let participantlist = {};
        const MEET_ID = "<%= meetId %>"
        let EmailId = "<%= EmailId %>"
        console.log(EmailId==="")
        if(EmailId==="") EmailId = window.prompt("Enter your Email ID", "abc@xyz.com");
    </script>
    <script src="https://unpkg.com/@popperjs/core@2/dist/umd/popper.min.js" defer></script>
    <script src="https://unpkg.com/tippy.js@6/dist/tippy-bundle.umd.js" defer></script>
    <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js" defer></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js" defer></script>
    <script src="/socket.io/socket.io.js" defer></script>
    <script src='meetingroom.js' defer></script>
    <script>
        
    </script>
    <title>Room</title>
    <link rel="stylesheet" type="text/css" href="/styles-room.css"/>
</head>
<body onload="addtolist()">
    <div class="header">
        <h4 id="logo">Teams</h4>
        <h4 id="email"></h4>
    </div>
    <div>
        <h5 class="username-head">User-Name</h5>
        <h3 class="username">Lorem Ipsum</h3>
        <button class="edit-name" onclick="editname()"></button>
    </div>
    <div>
        <button class='join-call' onclick="redirectcall()">Join call</button>
        <button class='leave-room' onclick="exitroom()">Leave Room</button>
        
    </div>
    <input id="message-ex" type="text" placeholder="Message" />
    <button id="send-ex"></button>
    <div class="chatbox-ex">
        <h4 id="msg-title">Messages</h4>
        <div id="messages-ex">
        </div>
        
    </div>
    <!-- <button class='invite'></button> -->
    <div class="info">
        <p><strong>Meeting Details</strong></p>
        <p><strong>URL:</strong></p>
        <p><strong>Meet Id:</strong></p>
    </div>
    <div class="participants" style="scroll-behavior: smooth;">
        <button class='invite'></button>
        <h4 class="part-heading">Participants</h4>
        <div class="part">
        </div>
    </div>
    
</body>
<script>
    document.getElementById("email").textContent = EmailId;
    let text=''
    text+='<p><strong>Meeting Details</strong></p>'
    text+='<p><strong>URL: </strong>https://khushi-vc-app.herokuapp.com/'+MEET_ID+'</p>'
    text+='<p><strong>Meet Id: </strong>'+MEET_ID+'</p>'
    document.querySelector(".info").innerHTML = text;
</script>
</html>